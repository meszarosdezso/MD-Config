    "[Vesper]": {
      "editorIndentGuide.background": "#0000",
      "editorIndentGuide.activeBackground": "#0000",
      "editorLineNumber.dimmedForeground": "#ff0000",
      "editorLineNumber.activeForeground": "#a8f0ea",
      "editorLineNumber.foreground": "#fff1",
      "editorGroup.border": "#fff1"
    }
  },
  "editor.tokenColorCustomizations": {
    "[GitHub Dark Default]": {
      "textMateRules": [
        {
          "name": "component tags",
          "scope": [
            "entity.name.tag", // HTML tags
            "support.class.component" // JSX/TSX Component tags
          ],
          "settings": {
            "foreground": "#99a8d4"
          }
        }
      ]
    },
    "[GitHub Dark Dimmed]": {
      "functions": "#95cbed",
      "variables": "#95cbed",
      "strings": "#fff2bc"
    },
    "[DuoTone Dark Space]": {
      "keywords": "#a5a5d6",
      "strings": "#ffa76c",
      "types": "#ffad65",
      "textMateRules": [
        {
          "scope": "variable.language",
          "settings": {
            "foreground": "#5c5c84"
          }
        }
      ]
    },
    "[Vesper]": {
      "strings": "#a8f0ea"
    }
  },
  "explorer.confirmDelete": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "extensions.ignoreRecommendations": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": -1
}
