      "sideBar.background": "#0b0c0f",
      "sideBarSectionHeader.background": "#0b0c0f",
      "sideBar.border": "#00000000",
      "editor.background": "#0b0c0f",
      "editorGroup.border": "#0000",
      "editorGroupHeader.tabsBorder": "#0000",
      "titleBar.activeBackground": "#0b0c0f",
      "sideBarSectionHeader.border": "#00000000",
      "gitDecoration.untrackedResourceForeground": "#e8ca8d",
      "gitDecoration.modifiedResourceForeground": "#90c0c9",
      "statusBar.border": "#0000",
