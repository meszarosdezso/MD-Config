{
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.iconTheme": "material-icon-theme",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "editor.fontFamily": "Operator Mono Lig, Jetbrains Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.useEditorConfig": false,
  "prettier.requireConfig": false,
  "deno.enable": false,
  "javascript.format.semicolons": "remove",
  "typescript.format.semicolons": "remove",
  "liveSassCompile.settings.generateMap": false,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "~/../css"
    }
  ],
  "window.zoomLevel": -1,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "git.enableSmartCommit": true,
  "explorer.confirmDragAndDrop": false,
  "editor.formatOnSave": true,
  "statusbar-format-on-save.formatString": "{{status}}",
  "statusbar-format-on-save.displayWhenTrue": "üëç",
  "statusbar-format-on-save.displayWhenFalse": "üö´",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.rulers": [82],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.suggest.insertMode": "replace"
  },
  "[rust]": {
    "editor.defaultFormatter": "statiolake.vscode-rustfmt"
  },
  "material-icon-theme.opacity": 0.8,
  "material-icon-theme.activeIconPack": "react",
  "debug.toolBarLocation": "docked",
  "dart_data_class_generator.toJson.enabled": false,
  "dart_data_class_generator.fromJson.enabled": false,
  "editor.wordWrap": "on",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "dart.lineLength": 82,
  "editor.wordWrapColumn": 82,
  "extensions.ignoreRecommendations": true,
  "diffEditor.ignoreTrimWhitespace": false
}
