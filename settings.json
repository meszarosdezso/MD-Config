{
  "workbench.colorTheme": "DuoTone Dark Space",
  "workbench.startupEditor": "none",
  "window.zoomLevel": -1,
  "editor.fontSize": 14,
  "git.enableSmartCommit": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.rulers": [100],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "workbench.colorCustomizations": {
    "[GitHub Dark Default]": {
      "activityBar.activeBackground": "#0b0c0f",
      "activityBar.background": "#0b0c0f",
      "activityBar.border": "#00000000",
      "titleBar.border": "#00000000",
      "tab.border": "#ffffff0a",
      "tab.activeBackground": "#0b0c0f",
      "tab.unfocusedActiveBorderTop": "#0000",
      "sideBar.background": "#0b0c0f",
      "sideBarSectionHeader.background": "#0b0c0f",
      "sideBar.border": "#00000000",
      "editor.background": "#0b0c0f",
      "editorGroup.border": "#0000",
      "editorGroupHeader.tabsBorder": "#0000",
      "titleBar.activeBackground": "#0b0c0f",
      "sideBarSectionHeader.border": "#00000000",
      "gitDecoration.untrackedResourceForeground": "#e8ca8d",
      "gitDecoration.modifiedResourceForeground": "#90c0c9",
      "statusBar.border": "#0000",
      "statusBar.debuggingBackground": "#253b47",
      "editorIndentGuide.activeBackground": "#0000",
      "editorIndentGuide.background": "#ffffff0f",
      "editorInlayHint.background": "#222630"
    },
    "[GitHub Light Default]": {
      "editor.background": "#fafdff",
      "activityBar.background": "#8ba3bf",
      "activityBar.activeBorder": "#3e647d",
      "activityBar.activeBackground": "#ff9a64",
      "activityBar.foreground": "#fff",
      "activityBar.inactiveForeground": "#fff8",
      "activityBar.border": "#00000000",
      "titleBar.border": "#00000000",
      "tab.border": "#ffffff0a",
      // "tab.activeBackground": "#0b0c0f",
      "tab.unfocusedActiveBorderTop": "#0000",
      // "sideBar.background": "#0b0c0f",
      // "sideBarSectionHeader.background": "#0b0c0f",
      "sideBar.border": "#00000000",
      // "editor.background": "#0b0c0f",
      "editorGroup.border": "#0000",
      "editorGroupHeader.tabsBorder": "#0000",
      // "titleBar.activeBackground": "#0b0c0f",
      "sideBarSectionHeader.border": "#00000000",
      // "gitDecoration.untrackedResourceForeground": "#e8ca8d",
      // "gitDecoration.modifiedResourceForeground": "#90c0c9",
      // "statusBar.border": "#0000",
      "editorIndentGuide.activeBackground": "#0000",
      "editorIndentGuide.background": "#0062ff0f"
      // "editorInlayHint.background": "#222630"
    },
    "[DuoTone Dark Space]": {
      "activityBar.background": "#1c1d25",
      "sideBar.background": "#1c1d25",
      "editor.background": "#1c1d25",
      "tab.border": "#0000",
      "tab.inactiveBackground": "#1c1d25",
      "window.activeBorder": "#0000",
      "widget.shadow": "#0000",
      "editorWidget.background": "#14141d",
      "activityBar.activeBackground": "#fe773411",
      "activityBar.activeBorder": "#fe7734",
      "titleBar.activeBackground": "#1c1d25",
      "editorGroupHeader.tabsBackground": "#1c1d25",
      "statusBar.background": "#1c1d25",
      "gitDecoration.modifiedResourceForeground": "#ff9058",
      "gitDecoration.untrackedResourceForeground": "#aeb5ff"
    }
  },
  "editor.tokenColorCustomizations": {
    "[GitHub Dark Dimmed]": {
      "functions": "#95cbed",
      "variables": "#95cbed",
      "strings": "#fff2bc"
    }
  },
  "explorer.confirmDelete": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "extensions.ignoreRecommendations": true,
  "dart.lineLength": 100
}
